{
  "name": "dlni-shared",
  "description": "",
  "version": "1.0.0",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\" --fix  --config .eslint.mjs",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma-gen": "npx prisma generate",
    "prisma-reset": "npx prisma migrate reset",
    "prisma-genmigrate": "npx prisma migrate dev --create-only",
    "prisma-push": "npx prisma db push",
    "prisma-migrate": "npx prisma migrate dev --name init",
    "prisma-deploy": "npx prisma migrate deploy",
    "prisma-studio": "npx prisma studio",
    "build": "tsc && tsc-alias"
  },
  "exports": {
    "./types/*": "./src/types/*.ts",
    "./utils/*": "./src/utils/*.ts",
    "./db/*": "./src/db/*.ts"
  },
