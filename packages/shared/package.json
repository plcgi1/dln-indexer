    "format": "^0.2.2",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@prisma/config": "^7.2.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
